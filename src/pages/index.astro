---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/Header.astro';
import Hero from '../components/Hero.astro';
import USPSection from '../components/USPSection.astro';
import AboutSection from '../components/AboutSection.astro';
import EquipmentSection from '../components/EquipmentSection.astro';
import Gallery from '../components/Gallery.astro';
import ReviewsSection from '../components/ReviewsSection.astro';
import ParallaxDivider from '../components/ParallaxDivider.astro';
import CalendarSection from '../components/CalendarSection.astro';
import LocationSection from '../components/LocationSection.astro';
import FAQSection from '../components/FAQSection.astro';
import ContactForm from '../components/ContactForm.astro';
import Footer from '../components/Footer.astro';
---

<BaseLayout>
  <Header />
  <main>
    <Hero />
    <USPSection />
    <AboutSection />
    <Gallery />
    <EquipmentSection />
    <ParallaxDivider />
    <LocationSection />
    <ReviewsSection />
    <CalendarSection />
    <FAQSection />
    <ContactForm />
  </main>
  <Footer />
</BaseLayout>
